cat /Users/apple/data/360/streaming_recommender/knowleage-graph/dataset_out/out_keyword.csv| awk -F "," '{gsub(/\"/, "", $2); print $2}' | sort | uniq -c | sort -rn  | awk '{if ($1 == 1) print $2}' > /Users/apple/data/360/streaming_recommender/knowleage-graph/dataset/keyword_not_related_words.txt
cat /Users/apple/data/360/streaming_recommender/knowleage-graph/dataset_out/out_keyword.csv| awk -F "," '{gsub(/\"/, "", $2); print $2}' | sort | uniq -c | sort -rn  | awk '{if ($1 == 2) print $2}' >>  /Users/apple/data/360/streaming_recommender/knowleage-graph/dataset/keyword_not_related_words.txt
cat /Users/apple/data/360/streaming_recommender/knowleage-graph/dataset_out/out_keyword.csv| awk -F "," '{gsub(/\"/, "", $2); print $2}' | sort | uniq -c | sort -rn  | awk '{if ($1 == 3) print $2}' >>  /Users/apple/data/360/streaming_recommender/knowleage-graph/dataset/keyword_not_related_words.txt